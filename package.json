{
	"name": "parcel-web-template",
	"version": "1.0.0",
	"main": "unrelated.js",
	"targets": {
		"main": false
	},
	"scripts": {
		"clean": "rm -rf .parcel-cache && rm -rf dist",
		"dev": "npm run clean && parcel ./src/*.html",
		"build": "parcel build ./src/*.html --no-source-maps --public-url ."
	},
	"author": "Bakhriev",
	"license": "ISC",
	"description": "Parcel web template",
	"devDependencies": {
		"@agragregra/parcel-namer-beautify": "^1.0.1",
		"@gatsbyjs/parcel-namer-relative-to-cwd": "^2.13.1",
		"@parcel/transformer-sass": "^2.12.0",
		"parcel": "^2.12.0",
		"posthtml-include": "^2.0.1",
		"prettier": "^3.3.3",
		"sharp": "^0.31.3"
	}
}
